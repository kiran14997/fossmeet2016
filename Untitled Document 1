package main
import (
        "fmt"
        "os"
        "strconv"
)
func main()  {
       i := os.args[1]
       n, err :=strconv.atoi(i)
       if err != nil  {
              fmt.println("nat a number",i)
              os.exit(1)
}
if n%2 ==0 {
          fmt.println("even number",n)
} else {
        fmt.println("odd number",n)
}
}
